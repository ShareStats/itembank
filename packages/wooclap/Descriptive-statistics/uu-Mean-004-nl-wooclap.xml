<answer fraction="0" format="html">
<text><![CDATA[<p>
<span class="math inline">\(\euro100\)</span>
</p>]]></text>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
<span class="math inline">\(\euro140\)</span>
</p>]]></text>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
<span class="math inline">\(\euro150\)</span>
</p>]]></text>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
<span class="math inline">\(\euro170\)</span>
</p>]]></text>
