<defaultgrade>1</defaultgrade>
<shuffleanswers>false</shuffleanswers>
<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Error variabele
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Mixed variabele
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Within-Subjects Factor
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
Between-Subjects Factor
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Correct
</p>]]></text>
</feedback>
</answer>
