<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
(3.5, 6.5) and (12.5, 15.5)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
(3.5, 6.5) and (12.5, 15.5): Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
(4, 6) and (12.5, 15.5)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
(4, 6) and (12.5, 15.5): Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
(2.5, 7.5) and (11.5, 16.5)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
(2.5, 7.5) and (11.5, 16.5): Correct
</p>]]></text>
</feedback>
</answer>
