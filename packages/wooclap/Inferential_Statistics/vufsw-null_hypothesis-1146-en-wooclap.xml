<shuffleanswers>false</shuffleanswers>
<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
H<sub>0</sub>: μ<sub>1 </sub>≠ μ<sub>2</sub>.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
False
</p>]]></text>
</feedback>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
H<sub>0</sub>: μ<sub>1</sub> - μ<sub>2 </sub>= 0.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
True
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
H<sub>0</sub>: μ<sub>1 </sub>= 0 and μ<sub>2</sub> = 0.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
False
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
H<sub>0</sub>: μ<sub>1 </sub>- μ<sub>2 </sub>≠ 0.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
False
</p>]]></text>
</feedback>
</answer>
