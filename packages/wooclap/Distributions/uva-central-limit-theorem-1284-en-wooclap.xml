<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
P(z = -1.55)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
P(z = -1.55): Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
P(z &lt; -1,55)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
P(z &lt; -1.55) Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
1 - P(z &lt; -1.55)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
1 - P(z &lt; -1.55) Correct
</p>]]></text>
