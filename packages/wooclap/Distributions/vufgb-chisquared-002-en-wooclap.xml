<penalty>0</penalty>
<defaultgrade>1</defaultgrade>
<shuffleanswers>false</shuffleanswers>
<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="100" format="html">
<text><![CDATA[<p>
The <span class="math inline">\(χ^2\)</span>-distribution cannot contain negative values.
</p>]]></text>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
The <span class="math inline">\(χ^2\)</span>-distribution is skewed to the left.
</p>]]></text>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
The <span class="math inline">\(χ^2\)</span>-distribution depends on two types of degrees of freedom.
</p>]]></text>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
The <span class="math inline">\(χ^2\)</span>-distribution is symmetric.
</p>]]></text>
