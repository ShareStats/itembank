<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
(49/151)5
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
(49/151)5: Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
