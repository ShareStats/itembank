<shuffleanswers>false</shuffleanswers>
<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Multivariate ANOVA (MANOVA)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Repeated measures ANOVA
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Covariance analysis (ANCOVA)
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
Linear mixed model
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Correct
</p>]]></text>
