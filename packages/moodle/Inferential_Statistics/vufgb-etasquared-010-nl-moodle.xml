<shuffleanswers>false</shuffleanswers>
<single>true</single>
<answernumbering>abc</answernumbering>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Dit is een klein effect.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Dit is een middelmatig effect.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
<answer fraction="100" format="html">
<text><![CDATA[<p>
Dit is een groot effect.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Correct
</p>]]></text>
</feedback>
</answer>
<answer fraction="0" format="html">
<text><![CDATA[<p>
Dit is een significant effect.
</p>]]></text>
<feedback format="html">
<text><![CDATA[<p>
Incorrect
</p>]]></text>
</feedback>
</answer>
